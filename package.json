  "name": "checkout-js",
  "description": "Internal package for the Razorpay Checkout JavaScript",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:razorpay/checkout.git"
  },
  "scripts": {
    "test:unit": "./rollup.test.js | tap-difflet",
