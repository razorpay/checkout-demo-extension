  "name": "checkout-js",
  "description": "Internal package for the Razorpay Checkout JavaScript",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:razorpay/checkout.git"
  },
  "scripts": {
    "start": "npm run clean && NODE_ENV=dev gulp watch",
    "dev-start": "API_SERVER=localhost nodemon server & npm start",
    "func-start": "API_SERVER=https://api-2.func.razorpay.in nodemon server & npm start",
    "live-start": "API_SERVER=https://api.razorpay.com nodemon server & npm start",
    "dark-start": "API_SERVER=https://api-dark.razorpay.com nodemon server & npm start",
    "start:no-reload": "gulp watch",
    "copy-1cc-checkout": "./scripts/copy_1cc_checkout.sh",
    "build": "npm run clean && rollup -c --environment prod && gulp && npm run copy-1cc-checkout && ./scripts/legacy.sh",
    "lint": "eslint app/modules",
    "lint-fix": "eslint app/modules --fix",
    "svelte-check": "svelte-check --tsconfig tsconfig.json --threshold error --output human",
    "clean": "./scripts/clean.sh",
    "jest:base": "jest blackbox/tests/homescreen/**/base.test.js",
    "jest:custom": "jest blackbox/tests/custom/**/*.test.js",
    "jest": "jest blackbox/tests/homescreen --testPathIgnorePatterns ersonalization",
    "jest:p13n": "jest blackbox/tests/homescreen/**/*ersonalization*.test.js --runInBand",
    "test:all": "npm run test:unit && npm run build && npm run jest && npm run jest:custom",
    "test:base": "npm run test:unit && npm run build && npm run jest:base",
    "test:unit": "NODE_ENV=test && jest --config jest.ut.config.js",
    "test:unit:1cc": "NODE_ENV=test && jest --config jest.1cc.ut.config.js",
    "test:unit:coverage": "NODE_ENV=test && __SVELTE_COVERAGE__=true jest --config jest.ut.config.js --coverage --outputFile test-results.json --json",
    "test:unit:1cc:coverage": "NODE_ENV=test && __SVELTE_COVERAGE__=true jest --config jest.1cc.ut.config.js --coverage --outputFile test-1cc-results.json --json 2>&1 | tee log-1cc.txt",
    "test": "npm run test:all",
    "vision": "./scripts/vision.sh",
    "vision:test": "npx playwright test",
    "postinstall": "chmod -R +x ./git-hooks/ && git config core.hooksPath ./git-hooks",
    "pretty-lint": "exec ./git-hooks/pre-commit"
  },
  "bugs": {
    "url": "https://github.com/razorpay/checkout/issues"
  },
  "private": true,
