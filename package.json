  "name": "checkout-js",
  "description": "Internal package for the Razorpay Checkout JavaScript",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:razorpay/checkout.git"
  },
  "scripts": {
    "start": "npm run clean && webpack serve --mode development",
    "func-start": "API_SERVER=https://api-2.func.razorpay.in npm start",
    "uat-start": "API_SERVER=https://api-uat.stage.razorpay.in npm start",
    "live-start": "API_SERVER=https://api.razorpay.com npm start",
    "dark-start": "API_SERVER=https://api-dark.razorpay.com npm start",
    "qa-start": "API_SERVER=https://api-3.qa.razorpay.in npm start",
    "start:autogen": "AUTOTEST_ANNOTATE=include npm start",
    "build": "npm run clean & webpack --mode production --node-env=production",
    "lint": "eslint app/modules",
    "lint-fix": "eslint app/modules --fix",
    "svelte-check": "svelte-check --tsconfig tsconfig.json --threshold error --output human",
    "clean": "./scripts/clean.sh",
    "jest:base": "jest blackbox/tests/homescreen/**/base.test.js",
    "jest:custom": "jest blackbox/tests/custom/**/*.test.js",
    "jest": "jest blackbox/tests/homescreen --testPathIgnorePatterns ersonalization",
    "jest:p13n": "jest blackbox/tests/homescreen/**/*ersonalization*.test.js --runInBand",
    "test:all": "npm run test:unit && npm run build && npm run jest && npm run jest:custom",
    "test:base": "npm run test:unit && npm run build && npm run jest:base",
    "test:unit": "NODE_ENV=test && jest --config jest.ut.config.js",
    "test:unit:1cc": "NODE_ENV=test && jest --config jest.1cc.ut.config.js",
    "test:unit:coverage": "NODE_ENV=test jest --config jest.ut.config.js --coverage --outputFile test-results.json --json",
    "test:unit:1cc:coverage": "NODE_ENV=test jest --config jest.1cc.ut.config.js --coverage --outputFile test-1cc-results.json --json 2>&1 | tee log-1cc.txt",
    "autotest": "node vision/autogen/index.mjs --remote",
    "autotest:record": "node vision/autogen/index.mjs --remote --record",
    "test": "npm run test:all",
    "vision": "./scripts/vision.sh",
    "vision:test": "npx playwright test",
    "prepare": "node ./scripts/prepare.js",
    "lint:staged": "lint-staged -c .lintstagedrc.json"
  },
  "bugs": {
    "url": "https://github.com/razorpay/checkout/issues"
  },
  "private": true,
