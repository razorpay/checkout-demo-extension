window.Razorpay = function(options){
  if(typeof this.configure == 'function')
    this.configure(options)
}
!function(){
  var RazorpayLibs = window.RazorpayLibs = {};
  var exports = null;
  var module = {exports: null};

  // jquery
  <%- include lib/jquery-1.11.1.js %>
  RazorpayLibs.$ = module.exports;

  // handlebars
  <%- include lib/handlebars-v2.0.0.js %>
  module.exports.registerHelper('toRupee', function(paise) {
    return parseInt(paise)/100;
  });
  RazorpayLibs.Handlebars = module.exports
  RazorpayLibs.templates = {}
}()
