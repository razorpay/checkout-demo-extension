      <label>IFSC</label>
      <input class="input" name="bank_account[ifsc]" type="text" id="adhr-acc-ifsc" value="`=bank_ifsc`"`?bank_ifsc` readonly`?` required pattern="^[a-zA-Z]{4}[0-9]{7}$" maxlength="11" spellcheck="false" autocorrect="off" autocapitalize="off">
    </div></div>
    <div class="elem-wrap" id="elem-wrap-adhr-acc-name"><div class="elem elem-adhr-acc-name`?bank_name` readonly`?`">
      <div class="help">Please enter a valid Name as per your account</div>
      <label>Name on Bank Account</label>
      <input class="input" name="bank_account[name]" type="text" id="adhr-acc-name" value="`=bank_name`"`?bank_name` readonly`?` required pattern="^[a-zA-Z. 0-9\']{1,100}$" maxlength="100" spellcheck="false" autocorrect="off" autocapitalize="off">
    </div></div>
    <div class="elem-wrap" id="elem-wrap-adhr-acc-aadhaar"><div class="elem elem-adhr-acc-aadhaar`?aadhaar` readonly`?`">
