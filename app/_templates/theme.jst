button{
  background: {{=it.color}};
}
#tab-bar{
  background: {{=it.color}};
}
.radio-label:hover, input[type=radio]:checked + label{
  border-color: {{=it.color}};
}
#tab-wallet input:checked+label:after{
  color: {{=it.color}};
}
.ip #tabs li.active{
  border-color: {{=it.color}};
}
{{?it.imagePadding}}
.merchant-image{
  width: 60px;
  height: 60px;
  padding: 12px;
  border: 1px solid #eaeaea;
  background: #fff;
}
{{?}}